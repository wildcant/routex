{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000 --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^1.2.5",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@types/leaflet": "^1.9.3",
    "@types/leaflet-draw": "^1.0.6",
    "autoprefixer": "^10.4.7",
    "config": "workspace:*",
    "database": "workspace:*",
    "eslint": "^8.28.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "type": "module",
  "dependencies": {
    "leaflet": "^1.9.3",
    "leaflet-draw": "^1.0.4"
  }
}
